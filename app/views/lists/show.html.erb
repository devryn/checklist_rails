<h2><%= list.name %></h2>
<% list.tasks.each do |t| %>
<p><%= link_to t.body, task_path(t) %>
  <% end %>

  <%= link_to "Edit", edit_list_path(list), class: "btn btn-default" %>
  <%= link_to "Delete", list, class: "btn btn-danger", method: :delete, data: { confirm: "Are you sure you want to delete this list and all its tasks?"} %>
